<Page x:Class="RockPaperScissors.FinalResults"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RockPaperScissors"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="300"
      Title="FinalResults">

    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="1"/>
                <GradientStop Color="#FF844040" Offset="0"/>
            </LinearGradientBrush>
        </Grid.Background>
        <TextBox Name="Results" TextWrapping="Wrap" Text="TextBox" Margin="10,10,10,110" FontFamily="Comic Sans MS" FontSize="35"/>
        <Button Content="Play Again" Margin="0,395,0,0" Background="#FF7ADE2B" Click="Restart"/>

    </Grid>
</Page>
