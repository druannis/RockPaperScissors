<Page x:Class="RockPaperScissors.FinalResults"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RockPaperScissors"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="300"
      Title="FinalResults">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/rpsbg.jpg"/>
        </Grid.Background>
        <TextBlock Name="Results" TextWrapping="Wrap" Text="TextBox" Margin="10,128,10,176" FontFamily="Comic Sans MS" FontSize="25" Background="#FFF3BB7C"/>
        <Button Content="Play Again" Margin="0,395,0,0" Background="#FFF3BB7C" Click="Restart"/>

    </Grid>
</Page>
